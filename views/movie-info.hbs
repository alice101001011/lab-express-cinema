

<div class="infos-body">
    <div class="card">
      <img src={{image}} class="card-img-top" alt={{title}} />
      <div class="card-body">
        <h3 class="card-title">{{title}}</h3>
        <h4>Director: {{director}}</h4>
        <h4>Stars</h4>
        <ul>
            {{#each stars}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p class="card-text">{{description}}</p>
        <p>Show times: {{showtimes}}</p>
        
      </div>
    </div>
  
</div>